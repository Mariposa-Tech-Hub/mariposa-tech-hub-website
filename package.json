{
  "name": "mariposa-tech-hub-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:serve": "concurrently --kill-others --names \"REACT,EXPRESS\" -c \"bgCyan.black,bgGreen.black\" \"vite build --watch --mode development\" \"npm run dev:api\"",
    "dev": "npm run build:ui && concurrently --kill-others --names \"REACT,EXPRESS\" -c \"bgCyan.black,bgGreen.black\" \"vite\" \"npm run dev:api\"",
    "dev:ui": "vite",
    "dev:api": "nodemon ./src/server.js",
    "build": "npm run build:api && npm run build:ui",
    "build:api": "echo api does not need to build!",
    "build:ui": "vite build",
    "preview": "vite preview",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix ."
  },
  "dependencies": {
    "@tanstack/react-router": "^0.0.1-beta.48",
    "axios": "^1.2.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
    "helmet": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0"
  }
}
